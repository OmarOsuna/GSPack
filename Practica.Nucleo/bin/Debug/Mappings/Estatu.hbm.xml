<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Practica.Nucleo.Entidades.Estatu, Practica.Nucleo"
         table="estatu" lazy="false">

    <id name="Id" type="int">
      <column name="id" />
      <generator class="identity" />
    </id>

    <property name="Fecha" type="DateTime">
      <column name="fecha" />
    </property>

    <property name="Descripcion" type="string">
      <column name="descripcion" />
    </property>
    
    <property name="TipoEstatu" type="Practica.Nucleo.Enumeradores.TipoEstatu, Practica.Nucleo">
      <column name="tipoestatu" />
    </property>


    <property name="Ciudad" type="string">
      <column name="ciudad" />
    </property>

    <property name="Estado" type="string">
      <column name="estado" />
    </property>

    <property name="Latitud" type="string">
      <column name="latitud" />
    </property>

    <property name="Longitud" type="string">
      <column name="longitud" />
    </property>  
  
  </class>
</hibernate-mapping>
