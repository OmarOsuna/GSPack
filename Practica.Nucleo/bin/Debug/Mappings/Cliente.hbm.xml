<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Practica.Nucleo.Entidades.Cliente, Practica.Nucleo"
         table="cliente" lazy="false">

    <id name="Id" type="int">
      <column name="id" />
      <generator class="identity" />
    </id>

    <property name="Nombre" type="string">
      <column name="nombre" />
    </property>

    <property name="TelefonoUno" type="string">
      <column name="telefonouno" />
    </property>

    <property name="TelefonoDos" type="string">
      <column name="telefonodos" />
    </property>

    <property name="TelefonoTres" type="string">
      <column name="telefonotres" />
    </property>

    <property name="Correo" type="string">
      <column name="correo" />
    </property>

    <property name="RFC" type="string">
      <column name="rfc" />
    </property>

  </class>
</hibernate-mapping>